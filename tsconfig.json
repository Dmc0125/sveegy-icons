{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "scripts/js",
    "rootDir": "scripts/lib",
    "resolveJsonModule": true
  },
  "exclude": ["packages"]
}
